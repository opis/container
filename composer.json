{
    "name": "opis/container",
    "description": "Serializable dependency injection container",
    "keywords": ["opis", "dependency", "injection", "container", "IoC", "serializable", "php7"],
    "homepage": "http://www.opis.io/container",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "opis/closure": "^2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*"
    },
    "autoload": {
        "psr-4": {"Opis\\Container\\": "lib/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Container\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
